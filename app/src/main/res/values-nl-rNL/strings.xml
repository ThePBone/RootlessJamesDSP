    <string name="onboarding_methods_rootless_adb">\u25CF Voor gevorderde gebruikers\n\u25CF Computer en USB kabel vereist\n\u25CF Android 10+ ondersteund</string>
    <!-- Root flavor -->
    <string name="onboarding_methods_root_root">\u25CF Aangeraden\n\u25CF Geen externe computer nodig\n\u25CF Alle Android versies</string>
